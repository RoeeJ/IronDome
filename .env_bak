# Seq Logging Configuration
# Enable Seq logging (set to 'true' to enable)
VITE_SEQ_ENABLED=true

# Direct Seq endpoint (e.g., http://localhost:5341 or https://your-seq-instance.com)
VITE_SEQ_ENDPOINT=https://seq.ptt.blue

# API key for direct Seq logging (required if not using proxy)
VITE_SEQ_API_KEY=aEsqllQzNjZBjcyf0jCO

# Use server proxy instead of direct logging (recommended for production)
VITE_SEQ_USE_PROXY=false

# Server proxy endpoint (if using proxy mode)
VITE_SEQ_PROXY_ENDPOINT=/api/logs/seq

# Batch size - number of logs to accumulate before sending
VITE_SEQ_BATCH_SIZE=100

# Batch timeout in milliseconds - max time to wait before sending batch
VITE_SEQ_BATCH_TIMEOUT=2000

# Debug mode - adds ?debug to enable console logging
# (Not needed for Seq, but kept for backward compatibility)
